{
  "name": "notbundle",
  "version": "0.1.1",
  "description": "Inspired by Vite's Not Bundle, building ts for use in Node.js.",
  "main": "index.mjs",
  "types": "types",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "repository": "https://github.com/caoxiemeihao/notbundle.git",
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc --emitDeclarationOnly && vite build",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "esbuild": "*"
  },
  "dependencies": {
    "chokidar": "^3.5.3",
    "fast-glob": "^3.2.12"
  },
  "devDependencies": {
    "@types/node": "^18.11.10",
    "esbuild": "^0.15.16",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  },
  "files": [
    "types",
    "index.mjs",
    "index.js"
  ],
  "keywords": [
    "unbundle",
    "bundle",
    "build"
  ]
}
